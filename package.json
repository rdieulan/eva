{
  "name": "eva",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config client/vite.config.ts",
    "server": "tsx --watch --tsconfig server/tsconfig.json server/index.ts",
    "build": "vue-tsc -b && vite build --config client/vite.config.ts && tsc -p server/tsconfig.json && tsc-alias -p server/tsconfig.json -f && npm run db:generate",
    "start": "NODE_ENV=production node dist-server/server/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/html2canvas": "^0.5.35",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "esbuild": "^0.27.2",
    "esbuild-register": "^3.6.0",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.4.0",
    "prisma": "^7.2.0",
    "sass": "^1.97.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.4"
  }
}
